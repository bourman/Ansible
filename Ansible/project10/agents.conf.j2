# Sample agents.conf generated by Ansible
{% for host in groups['lamp_app'] %}
agent {{
    host = "{{ hostvars[host]['ansible_host'] }}"
    port = "{{ hostvars[host]['ansible_port'] }}"
    username = "{{ hostvars[host]['ansible_user'] }}"
    password = "{{ hostvars[host]['ansible_password'] }}"
}}
{% endfor %}
